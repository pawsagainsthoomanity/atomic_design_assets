<ol class="ads-c-breadcrumbs">
    {{# breadcrumbs }}
    <li class="ads-c-breadcrumbs__item">
    	<a href={{ url }} class="ads-c-breadcrumbs__link">
            
            {{# breadcrumbsIconBefore }}
            <svg class="ads-c-icon ads-c-breadcrumbs__icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="{{ iconPath }}#{{ breadcrumbsIconBefore }}"></use>
            </svg>
            {{/ breadcrumbsIconBefore }}

            {{# label }}
            <span class="ads-c-breadcrumbs__text">{{ label }}</span>
            {{/ label }}
        </a>
    </li>
    {{/ breadcrumbs }}
</ol>
